{
      "$defs": {
            "CreativeWork": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "CreativeWork"
                        },
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "The name of the item."
                        },
                        "abstract": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "An abstract is a short description that summarizes a CreativeWork.",
                              "title": "abstract"
                        },
                        "accessMode": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The human sensory perceptual system or cognitive faculty through which a person may process or perceive information. Values should be drawn from the approved vocabulary.",
                              "title": "Access Mode"
                        },
                        "accessibilitySummary": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "A human-readable summary of specific accessibility features or deficiencies, consistent with the other accessibility metadata but expressing subtleties such as \"short descriptions are present but long descriptions will be needed for non-visual users\" or \"short descriptions are present and no long descriptions are needed.\"",
                              "title": "Acces Summary"
                        },
                        "identifier": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links."
                        },
                        "creator": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Person"
                                    },
                                    {
                                          "$ref": "#/$defs/Organization"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork.",
                              "title": "Creative Work creator"
                        },
                        "publisher": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Person"
                                    },
                                    {
                                          "$ref": "#/$defs/Organization"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The publisher of the creative work.",
                              "title": "Creative Work publisher"
                        },
                        "countryOfOrigin": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The country of origin of something, including products as well as creative works such as movie and TV content.\n\nIn the case of TV and movie, this would be the country of the principle offices of the production company or individual responsible for the movie. For other kinds of CreativeWork it is difficult to provide fully general guidance, and properties such as contentLocation and locationCreated may be more applicable.\n\nIn the case of products, the country of origin of the product. The exact interpretation of this may vary by context and product type, and cannot be fully enumerated here.",
                              "title": "Country Of Origin"
                        },
                        "jurisdiction": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Indicates a legal jurisdiction, e.g. of some legislation, or where some government service is based.",
                              "title": "Jurisdiction"
                        },
                        "typicalAgeRange": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": null,
                              "description": "The typical expected age range, e.g. '7-9', '11-'."
                        },
                        "temporalCoverage": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The typical expected age range, e.g. '7-9', '11-'.",
                              "title": "temporalCoverage"
                        }
                  },
                  "required": [
                        "name"
                  ],
                  "title": "CreativeWork",
                  "type": "object"
            },
            "DataCatalog": {
                  "description": "This is pretty incomplete, but we might not need it... ",
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "DataCatalog"
                        },
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "The name of the item."
                        }
                  },
                  "required": [
                        "name"
                  ],
                  "title": "DataCatalog",
                  "type": "object"
            },
            "DataDownload": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "DataDownload"
                        },
                        "contentUrl": {
                              "description": "Actual bytes of the media object, for example the image file or video file.",
                              "format": "uri",
                              "minLength": 1,
                              "title": "Contenturl",
                              "type": "string"
                        },
                        "encodingFormat": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "items": {
                                                "$ref": "#/$defs/Text"
                                          },
                                          "type": "array"
                                    },
                                    {
                                          "items": {
                                                "format": "uri",
                                                "minLength": 1,
                                                "type": "string"
                                          },
                                          "type": "array"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": "Unknown",
                              "description": "Media type typically expressed using a MIME format (see IANA site and MDN reference), e.g. application/zip for a SoftwareApplication binary, audio/mpeg for .mp3 etc. \nIn cases where a CreativeWork has several media type representations, encoding can be used to indicate each MediaObject alongside particular encodingFormat information.\n\nUnregistered or niche encoding and file formats can be indicated instead via the most appropriate URL, e.g. defining Web page or a Wikipedia/Wikidata entry. Supersedes fileFormat.",
                              "examples": [
                                    "CSV",
                                    "XML"
                              ],
                              "title": "Encodingformat"
                        }
                  },
                  "required": [
                        "contentUrl"
                  ],
                  "title": "DataDownload",
                  "type": "object"
            },
            "Number": {
                  "title": "Number",
                  "type": "string"
            },
            "Organization": {
                  "properties": {
                        "name": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Name of the organisation",
                              "example": "Fictitious Research Consortium"
                        },
                        "identifier": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See background notes for more details."
                        },
                        "sameAs": {
                              "anyOf": [
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
                              "example": "https://www.kaggle.com/datasets/meirnizri/covid19-dataset",
                              "title": "Sameas"
                        },
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Organization"
                        },
                        "email": {
                              "anyOf": [
                                    {
                                          "format": "email",
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Email address.",
                              "title": "Email"
                        },
                        "legalName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Legal name of the organisation",
                              "example": "Fictitious Research Consortium"
                        }
                  },
                  "title": "Organization",
                  "type": "object"
            },
            "Person": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Person"
                        },
                        "sameAs": {
                              "anyOf": [
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
                              "example": "https://orcid.org/0000-0000-0000-0000",
                              "title": "Sameas"
                        },
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "Full name of a person",
                              "example": "Jane Foo"
                        },
                        "givenName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Given name. In the U.S., the first name of a Person.",
                              "example": "Jane"
                        },
                        "familyName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Family name. In the U.S., the last name of a Person.",
                              "example": "Foo"
                        },
                        "email": {
                              "anyOf": [
                                    {
                                          "format": "email",
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Email address",
                              "example": "blah@google.com",
                              "title": "Email"
                        },
                        "affiliation": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Organization"
                                    }
                              ],
                              "description": "An organization that this person is affiliated with. For example, a school/university, a club, or a team."
                        }
                  },
                  "required": [
                        "sameAs",
                        "name",
                        "givenName",
                        "familyName",
                        "email",
                        "affiliation"
                  ],
                  "title": "Person",
                  "type": "object"
            },
            "Text": {
                  "title": "Text",
                  "type": "string"
            }
      },
      "additionalProperties": false,
      "properties": {
            "@type": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "Dataset"
            },
            "name": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "description": "The name of the item.",
                  "title": "Name"
            },
            "identifier": {
                  "anyOf": [
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "items": {
                                    "anyOf": [
                                          {
                                                "format": "uri",
                                                "minLength": 1,
                                                "type": "string"
                                          },
                                          {
                                                "$ref": "#/$defs/Text"
                                          }
                                    ]
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "An identifier, such as a DOI or a Compact Identifier. If the dataset has more than one identifier, repeat the identifier property. If using JSON-LD, this is represented using JSON list syntax.",
                  "title": "identifier"
            },
            "creator": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Organization"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork.",
                  "title": "Dataset Creator"
            },
            "publisher": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Person"
                        },
                        {
                              "$ref": "#/$defs/Organization"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The publisher of the creative work.",
                  "title": "Publisher"
            },
            "@id": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "Another ID for context"
            },
            "@context": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "https://schema.org/"
            },
            "description": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "description": "A description of the item.",
                  "title": "description"
            },
            "distribution": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/DataDownload"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A downloadable form of this dataset, at a specific location, in a specific format. This property can be repeated if different variations are available. There is no expectation that different downloadable distributions must contain exactly equivalent information (see also DCAT on this point). Different distributions might include or exclude different subsets of the entire dataset, for example",
                  "title": "distribution"
            },
            "includedInDataCatalog": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/DataCatalog"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A data catalog which contains this dataset. Supersedes includedDataCatalog, catalog. Inverse property: dataset",
                  "title": "Included In Data Catalog"
            },
            "isBasedOn": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Use to link a Dataset to the Study that it was generated from. A resource that was used in the creation of this resource. This term can be repeated for multiple sources. For example, http://example.com/great-multiplication-intro.html. Supersedes isBasedOnUrl.",
                  "title": "Is Based On"
            },
            "alternateName": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "items": {
                                    "$ref": "#/$defs/Text"
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Alternative names that have been used to refer to this dataset, such as aliases or abbreviations",
                  "example": [
                        "Quick Draw Dataset",
                        "quickdraw-dataset"
                  ],
                  "title": "Alternate Name"
            },
            "citation": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A citation or reference to another creative work, such as another publication, web page, scholarly article, etc.",
                  "title": "citation"
            },
            "isPartOf": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "If the dataset is a collection of smaller datasets, use the hasPart property to denote such relationship. Conversly, if the dataset is part of a larger dataset, use isPartOf. Both properties can take the form of a URL or a Dataset instance. In case Dataset is used as a value it has to include all of the properties required for a standalone Dataset. ",
                  "examples": [
                        "https://example.com/aggregate_dataset"
                  ],
                  "title": "Is Part Of (dataset)"
            },
            "hasPart": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Indicates an item or CreativeWork that is part of this item, or CreativeWork (in some sense).",
                  "title": "Has Part"
            },
            "isAccessibleForFree": {
                  "anyOf": [
                        {
                              "type": "boolean"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Whether the dataset is accessible without payment.",
                  "title": "Is Accessible For Free?"
            },
            "keywords": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "items": {
                                    "$ref": "#/$defs/Text"
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Keywords summarizing the dataset.",
                  "examples": [
                        [
                              "ATMOSPHERE > ATMOSPHERIC PHENOMENA > CYCLONES",
                              "ATMOSPHERE > ATMOSPHERIC PHENOMENA > DROUGHT",
                              "ATMOSPHERE > ATMOSPHERIC PHENOMENA > FOG",
                              "ATMOSPHERE > ATMOSPHERIC PHENOMENA > FREEZE"
                        ]
                  ],
                  "title": "Keywords"
            },
            "license": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A license under which the dataset is distributed. Provide a URL that unambiguously identifies a specific version of the license used. e.g. https://creativecommons.org/licenses/by/4.0 not https://creativecommons.org/licenses/by",
                  "examples": [
                        "https://creativecommons.org/publicdomain/zero/1.0/",
                        {
                              "@type": "CreativeWork",
                              "name": "Custom license",
                              "url": "https://example.com/custom_license"
                        }
                  ],
                  "title": "License"
            },
            "measurementTechnique": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The technique, technology, or methodology used in a dataset, which can correspond to the variable(s) described in variableMeasured.",
                  "title": "Measurement Technique"
            },
            "sameAs": {
                  "anyOf": [
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "items": {
                                    "format": "uri",
                                    "minLength": 1,
                                    "type": "string"
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The URL of a reference web page that unambiguously indicates the dataset's identity.",
                  "title": "Same As (dataset)"
            },
            "variableMeasured": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The variable that this dataset measures. For example, temperature or pressure.",
                  "title": "Variable Measured"
            },
            "version": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "$ref": "#/$defs/Number"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The version number for the dataset.",
                  "title": "version"
            },
            "url": {
                  "anyOf": [
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Location of a page describing the dataset.",
                  "title": "url"
            },
            "dateCreated": {
                  "anyOf": [
                        {
                              "format": "date",
                              "type": "string"
                        },
                        {
                              "format": "date-time",
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The date on which the CreativeWork was created or the item was added to a DataFeed.",
                  "title": "Date Created"
            },
            "dateModified": {
                  "anyOf": [
                        {
                              "format": "date",
                              "type": "string"
                        },
                        {
                              "format": "date-time",
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The date on which the CreativeWork was most recently modified or when the item's entry was modified within a DataFeed.",
                  "title": "Date (last) Modified"
            },
            "datePublished": {
                  "anyOf": [
                        {
                              "format": "date",
                              "type": "string"
                        },
                        {
                              "format": "date-time",
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Date of first broadcast/publication.",
                  "title": "Date Published"
            },
            "maintainer": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Person"
                        },
                        {
                              "$ref": "#/$defs/Organization"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A maintainer of a Dataset, software package (SoftwareApplication), or other Project. A maintainer is a Person or Organization that manages contributions to, and/or publication of, some (typically complex) artifact. It is common for distributions of software and data to be based on 'upstream' sources. When maintainer is applied to a specific version of something e.g. a particular version or packaging of a Dataset, it is always possible that the upstream source has a different maintainer. The isBasedOn property can be used to indicate such relationships between datasets to make the different maintenance roles clear. Similarly in the case of software, a package may have dedicated maintainers working on integration into software distributions such as Ubuntu, as well as upstream maintainers of the underlying work.",
                  "title": "Maintainer"
            },
            "dct:conformsTo": {
                  "default": {
                        "http://purl.org/dc/terms/conformsTo": {
                              "@id": "https://bioschemas.org/profiles/Dataset/1.1-DRAFT",
                              "@type": "CreativeWork"
                        }
                  },
                  "title": "Dct:Conformsto",
                  "type": "object"
            }
      },
      "required": [
            "name",
            "description"
      ],
      "title": "Dataset",
      "type": "object"
}