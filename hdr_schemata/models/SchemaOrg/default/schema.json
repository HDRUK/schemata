{
      "$defs": {
            "CreativeWork": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "CreativeWork"
                        },
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "The name of the item."
                        },
                        "abstract": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "An abstract is a short description that summarizes a CreativeWork.",
                              "title": "abstract"
                        },
                        "accessMode": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The human sensory perceptual system or cognitive faculty through which a person may process or perceive information. Values should be drawn from the approved vocabulary.",
                              "title": "Access Mode"
                        },
                        "accessibilitySummary": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "A human-readable summary of specific accessibility features or deficiencies, consistent with the other accessibility metadata but expressing subtleties such as \"short descriptions are present but long descriptions will be needed for non-visual users\" or \"short descriptions are present and no long descriptions are needed.\"",
                              "title": "Acces Summary"
                        },
                        "identifier": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links."
                        },
                        "creator": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Person"
                                    },
                                    {
                                          "$ref": "#/$defs/Organization"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork.",
                              "title": "Creative Work creator"
                        },
                        "publisher": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Person"
                                    },
                                    {
                                          "$ref": "#/$defs/Organization"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The publisher of the creative work.",
                              "title": "Creative Work publisher"
                        }
                  },
                  "required": [
                        "name"
                  ],
                  "title": "CreativeWork",
                  "type": "object"
            },
            "DataCatalog": {
                  "description": "This is pretty incomplete, but we might not need it... ",
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "DataCatalog"
                        },
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "The name of the item."
                        }
                  },
                  "required": [
                        "name"
                  ],
                  "title": "DataCatalog",
                  "type": "object"
            },
            "DataDownload": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "DataDownload"
                        },
                        "contentUrl": {
                              "description": "Actual bytes of the media object, for example the image file or video file.",
                              "format": "uri",
                              "minLength": 1,
                              "title": "Contenturl",
                              "type": "string"
                        },
                        "encodingFormat": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "items": {
                                                "$ref": "#/$defs/Text"
                                          },
                                          "type": "array"
                                    },
                                    {
                                          "items": {
                                                "format": "uri",
                                                "minLength": 1,
                                                "type": "string"
                                          },
                                          "type": "array"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": "Unknown",
                              "description": "Media type typically expressed using a MIME format (see IANA site and MDN reference), e.g. application/zip for a SoftwareApplication binary, audio/mpeg for .mp3 etc. \nIn cases where a CreativeWork has several media type representations, encoding can be used to indicate each MediaObject alongside particular encodingFormat information.\n\nUnregistered or niche encoding and file formats can be indicated instead via the most appropriate URL, e.g. defining Web page or a Wikipedia/Wikidata entry. Supersedes fileFormat.",
                              "examples": [
                                    "CSV",
                                    "XML"
                              ],
                              "title": "Encodingformat"
                        }
                  },
                  "required": [
                        "contentUrl"
                  ],
                  "title": "DataDownload",
                  "type": "object"
            },
            "GeoCoordinates": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "GeoCoordinates"
                        },
                        "latitude": {
                              "pattern": "^[-]?([0-8]?[0-9]|90)(\\.[0-9]+)?$",
                              "title": "Latitude",
                              "type": "string"
                        },
                        "longitude": {
                              "pattern": "^[-]?((1[0-7][0-9])|([0-9]?[0-9]))(\\.[0-9]+)?$",
                              "title": "Longitude",
                              "type": "string"
                        }
                  },
                  "required": [
                        "latitude",
                        "longitude"
                  ],
                  "title": "GeoCoordinates",
                  "type": "object"
            },
            "Number": {
                  "title": "Number",
                  "type": "string"
            },
            "OpenEndedTimePeriod": {
                  "pattern": "^\\d{4}-\\d{2}-\\d{2}\\/\\.\\.$",
                  "title": "OpenEndedTimePeriod",
                  "type": "string"
            },
            "Organization": {
                  "properties": {
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "Name of the organisation",
                              "example": "Fictitious Research Consortium"
                        },
                        "identifier": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See background notes for more details."
                        },
                        "sameAs": {
                              "anyOf": [
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
                              "example": "https://www.kaggle.com/datasets/meirnizri/covid19-dataset",
                              "title": "Sameas"
                        },
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Organization"
                        },
                        "email": {
                              "anyOf": [
                                    {
                                          "format": "email",
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Email address.",
                              "title": "Email"
                        },
                        "legalName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Legal name of the organisation",
                              "example": "Fictitious Research Consortium"
                        }
                  },
                  "required": [
                        "name"
                  ],
                  "title": "Organization",
                  "type": "object"
            },
            "Person": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Person"
                        },
                        "sameAs": {
                              "anyOf": [
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
                              "example": "https://orcid.org/0000-0000-0000-0000",
                              "title": "Sameas"
                        },
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "Full name of a person",
                              "example": "Jane Foo"
                        },
                        "givenName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Given name. In the U.S., the first name of a Person.",
                              "example": "Jane"
                        },
                        "familyName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Family name. In the U.S., the last name of a Person.",
                              "example": "Foo"
                        },
                        "email": {
                              "anyOf": [
                                    {
                                          "format": "email",
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Email address",
                              "example": "blah@google.com",
                              "title": "Email"
                        },
                        "affiliation": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Organization"
                                    }
                              ],
                              "description": "An organization that this person is affiliated with. For example, a school/university, a club, or a team."
                        }
                  },
                  "required": [
                        "sameAs",
                        "name",
                        "givenName",
                        "familyName",
                        "email",
                        "affiliation"
                  ],
                  "title": "Person",
                  "type": "object"
            },
            "Place": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Place"
                        },
                        "geo": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/GeoCoordinates"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "GeoCoordinates of the place"
                        }
                  },
                  "title": "Place",
                  "type": "object"
            },
            "SingleDate": {
                  "format": "date",
                  "title": "SingleDate",
                  "type": "string"
            },
            "Text": {
                  "title": "Text",
                  "type": "string"
            },
            "TimePeriod": {
                  "pattern": "^\\d{4}-\\d{2}-\\d{2}\\/\\d{4}-\\d{2}-\\d{2}$",
                  "title": "TimePeriod",
                  "type": "string"
            }
      },
      "additionalProperties": false,
      "properties": {
            "@type": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "Dataset"
            },
            "name": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "description": "The name of the item.",
                  "title": "Name"
            },
            "abstract": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "An abstract is a short description that summarizes a CreativeWork.",
                  "title": "abstract"
            },
            "accessMode": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The human sensory perceptual system or cognitive faculty through which a person may process or perceive information. Values should be drawn from the approved vocabulary.",
                  "title": "Access Mode"
            },
            "accessibilitySummary": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A human-readable summary of specific accessibility features or deficiencies, consistent with the other accessibility metadata but expressing subtleties such as \"short descriptions are present but long descriptions will be needed for non-visual users\" or \"short descriptions are present and no long descriptions are needed.\"",
                  "title": "Acces Summary"
            },
            "identifier": {
                  "anyOf": [
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "items": {
                                    "anyOf": [
                                          {
                                                "format": "uri",
                                                "minLength": 1,
                                                "type": "string"
                                          },
                                          {
                                                "$ref": "#/$defs/Text"
                                          }
                                    ]
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "An identifier, such as a DOI or a Compact Identifier. If the dataset has more than one identifier, repeat the identifier property. If using JSON-LD, this is represented using JSON list syntax.",
                  "title": "identifier"
            },
            "creator": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Organization"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork.",
                  "title": "Dataset Creator"
            },
            "publisher": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Person"
                        },
                        {
                              "$ref": "#/$defs/Organization"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The publisher of the creative work.",
                  "title": "Publisher"
            },
            "@id": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "Another ID for context"
            },
            "@context": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "https://schema.org/"
            },
            "description": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "description": "A description of the item.",
                  "title": "description"
            },
            "distribution": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/DataDownload"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A downloadable form of this dataset, at a specific location, in a specific format. This property can be repeated if different variations are available. There is no expectation that different downloadable distributions must contain exactly equivalent information (see also DCAT on this point). Different distributions might include or exclude different subsets of the entire dataset, for example",
                  "title": "distribution"
            },
            "includedInDataCatalog": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/DataCatalog"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A data catalog which contains this dataset. Supersedes includedDataCatalog, catalog. Inverse property: dataset",
                  "title": "Included In Data Catalog"
            },
            "isBasedOn": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Use to link a Dataset to the Study that it was generated from. A resource that was used in the creation of this resource. This term can be repeated for multiple sources. For example, http://example.com/great-multiplication-intro.html. Supersedes isBasedOnUrl.",
                  "title": "Is Based On"
            },
            "alternateName": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "items": {
                                    "$ref": "#/$defs/Text"
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Alternative names that have been used to refer to this dataset, such as aliases or abbreviations",
                  "example": [
                        "Quick Draw Dataset",
                        "quickdraw-dataset"
                  ],
                  "title": "Alternate Name"
            },
            "citation": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A citation or reference to another creative work, such as another publication, web page, scholarly article, etc.",
                  "title": "citation"
            },
            "funder": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Organization"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A person or organization that supports (sponsors) something through some kind of financial contribution.",
                  "title": "Funder"
            },
            "isPartOf": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "If the dataset is a collection of smaller datasets, use the hasPart property to denote such relationship. Conversly, if the dataset is part of a larger dataset, use isPartOf. Both properties can take the form of a URL or a Dataset instance. In case Dataset is used as a value it has to include all of the properties required for a standalone Dataset. ",
                  "examples": [
                        "https://example.com/aggregate_dataset"
                  ],
                  "title": "Is Part Of (dataset)"
            },
            "hasPart": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Indicates an item or CreativeWork that is part of this item, or CreativeWork (in some sense).",
                  "title": "Has Part"
            },
            "isAccessibleForFree": {
                  "anyOf": [
                        {
                              "type": "boolean"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Whether the dataset is accessible without payment.",
                  "title": "Is Accessible For Free?"
            },
            "keywords": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "items": {
                                    "$ref": "#/$defs/Text"
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Keywords summarizing the dataset.",
                  "examples": [
                        [
                              "ATMOSPHERE > ATMOSPHERIC PHENOMENA > CYCLONES",
                              "ATMOSPHERE > ATMOSPHERIC PHENOMENA > DROUGHT",
                              "ATMOSPHERE > ATMOSPHERIC PHENOMENA > FOG",
                              "ATMOSPHERE > ATMOSPHERIC PHENOMENA > FREEZE"
                        ]
                  ],
                  "title": "Keywords"
            },
            "license": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A license under which the dataset is distributed. Provide a URL that unambiguously identifies a specific version of the license used. e.g. https://creativecommons.org/licenses/by/4.0 not https://creativecommons.org/licenses/by",
                  "examples": [
                        "https://creativecommons.org/publicdomain/zero/1.0/",
                        {
                              "@type": "CreativeWork",
                              "name": "Custom license",
                              "url": "https://example.com/custom_license"
                        }
                  ],
                  "title": "License"
            },
            "measurementTechnique": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The technique, technology, or methodology used in a dataset, which can correspond to the variable(s) described in variableMeasured.",
                  "title": "Measurement Technique"
            },
            "sameAs": {
                  "anyOf": [
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "items": {
                                    "format": "uri",
                                    "minLength": 1,
                                    "type": "string"
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The URL of a reference web page that unambiguously indicates the dataset's identity.",
                  "title": "Same As (dataset)"
            },
            "spatialCoverage": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "$ref": "#/$defs/Place"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "You can provide a single point that describes the spatial aspect of the dataset. Only include this property if the dataset has a spatial dimension. For example, a single point where all the measurements were collected, or the coordinates of a bounding box for an area.",
                  "examples": [
                        {
                              "@type": "Place",
                              "geo": {
                                    "@type": "GeoShape",
                                    "box": "39.3280 120.1633 40.445 123.7878"
                              }
                        }
                  ],
                  "title": "Spatial Coverage"
            },
            "temporalCoverage": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/SingleDate"
                        },
                        {
                              "$ref": "#/$defs/TimePeriod"
                        },
                        {
                              "$ref": "#/$defs/OpenEndedTimePeriod"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The data in the dataset covers a specific time interval. Only include this property if the dataset has a temporal dimension. Schema.org uses the ISO 8601 standard to describe time intervals and time points. You can describe dates differently depending upon the dataset interval. Indicate open-ended intervals with two decimal points (..).",
                  "title": "Temporal Coverage"
            },
            "variableMeasured": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The variable that this dataset measures. For example, temperature or pressure.",
                  "title": "Variable Measured"
            },
            "version": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "$ref": "#/$defs/Number"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The version number for the dataset.",
                  "title": "version"
            },
