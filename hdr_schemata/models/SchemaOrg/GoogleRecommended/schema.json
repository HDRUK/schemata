{
      "$defs": {
            "CreativeWork": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "CreativeWork"
                        },
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "The name of the item."
                        },
                        "abstract": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "An abstract is a short description that summarizes a CreativeWork.",
                              "title": "abstract"
                        },
                        "accessMode": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The human sensory perceptual system or cognitive faculty through which a person may process or perceive information. Values should be drawn from the approved vocabulary.",
                              "title": "Access Mode"
                        },
                        "accessibilitySummary": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "A human-readable summary of specific accessibility features or deficiencies, consistent with the other accessibility metadata but expressing subtleties such as \"short descriptions are present but long descriptions will be needed for non-visual users\" or \"short descriptions are present and no long descriptions are needed.\"",
                              "title": "Acces Summary"
                        },
                        "identifier": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links."
                        },
                        "creator": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Person"
                                    },
                                    {
                                          "$ref": "#/$defs/hdr_schemata__models__SchemaOrg__base__Organization__Organization"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork.",
                              "title": "Creative Work creator"
                        },
                        "publisher": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Person"
                                    },
                                    {
                                          "$ref": "#/$defs/hdr_schemata__models__SchemaOrg__base__Organization__Organization"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The publisher of the creative work.",
                              "title": "Creative Work publisher"
                        },
                        "countryOfOrigin": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The country of origin of something, including products as well as creative works such as movie and TV content.\n\nIn the case of TV and movie, this would be the country of the principle offices of the production company or individual responsible for the movie. For other kinds of CreativeWork it is difficult to provide fully general guidance, and properties such as contentLocation and locationCreated may be more applicable.\n\nIn the case of products, the country of origin of the product. The exact interpretation of this may vary by context and product type, and cannot be fully enumerated here.",
                              "title": "Country Of Origin"
                        }
                  },
                  "required": [
                        "name"
                  ],
                  "title": "CreativeWork",
                  "type": "object"
            },
            "DataDownload": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "DataDownload"
                        },
                        "contentUrl": {
                              "description": "Actual bytes of the media object, for example the image file or video file.",
                              "format": "uri",
                              "minLength": 1,
                              "title": "Contenturl",
                              "type": "string"
                        },
                        "encodingFormat": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "items": {
                                                "$ref": "#/$defs/Text"
                                          },
                                          "type": "array"
                                    },
                                    {
                                          "items": {
                                                "format": "uri",
                                                "minLength": 1,
                                                "type": "string"
                                          },
                                          "type": "array"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": "Unknown",
                              "description": "Media type typically expressed using a MIME format (see IANA site and MDN reference), e.g. application/zip for a SoftwareApplication binary, audio/mpeg for .mp3 etc. \nIn cases where a CreativeWork has several media type representations, encoding can be used to indicate each MediaObject alongside particular encodingFormat information.\n\nUnregistered or niche encoding and file formats can be indicated instead via the most appropriate URL, e.g. defining Web page or a Wikipedia/Wikidata entry. Supersedes fileFormat.",
                              "examples": [
                                    "CSV",
                                    "XML"
                              ],
                              "title": "Encodingformat"
                        }
                  },
                  "required": [
                        "contentUrl"
                  ],
                  "title": "DataDownload",
                  "type": "object"
            },
            "GeoCoordinates": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "GeoCoordinates"
                        },
                        "latitude": {
                              "pattern": "^[-]?([0-8]?[0-9]|90)(\\.[0-9]+)?$",
                              "title": "Latitude",
                              "type": "string"
                        },
                        "longitude": {
                              "pattern": "^[-]?((1[0-7][0-9])|([0-9]?[0-9]))(\\.[0-9]+)?$",
                              "title": "Longitude",
                              "type": "string"
                        }
                  },
                  "required": [
                        "latitude",
                        "longitude"
                  ],
                  "title": "GeoCoordinates",
                  "type": "object"
            },
            "Number": {
                  "title": "Number",
                  "type": "string"
            },
            "OpenEndedTimePeriod": {
                  "pattern": "^\\d{4}-\\d{2}-\\d{2}\\/\\.\\.$",
                  "title": "OpenEndedTimePeriod",
                  "type": "string"
            },
            "Person": {
                  "properties": {
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Person"
                        },
                        "sameAs": {
                              "anyOf": [
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
                              "example": "https://orcid.org/0000-0000-0000-0000",
                              "title": "Sameas"
                        },
                        "name": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "description": "Full name of a person",
                              "example": "Jane Foo"
                        },
                        "givenName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Given name. In the U.S., the first name of a Person.",
                              "example": "Jane"
                        },
                        "familyName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Family name. In the U.S., the last name of a Person.",
                              "example": "Foo"
                        },
                        "email": {
                              "anyOf": [
                                    {
                                          "format": "email",
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "description": "Email address",
                              "example": "blah@google.com",
                              "title": "Email"
                        },
                        "affiliation": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/hdr_schemata__models__SchemaOrg__base__Organization__Organization"
                                    }
                              ],
                              "description": "An organization that this person is affiliated with. For example, a school/university, a club, or a team."
                        }
                  },
                  "required": [
                        "sameAs",
                        "name",
                        "givenName",
                        "familyName",
                        "email",
                        "affiliation"
                  ],
                  "title": "Person",
                  "type": "object"
            },
            "Place": {
                  "properties": {
                        "name": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Name of the organisation",
                              "example": "Fictitious Research Consortium"
                        },
                        "identifier": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See background notes for more details."
                        },
                        "sameAs": {
                              "anyOf": [
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
                              "example": "https://www.kaggle.com/datasets/meirnizri/covid19-dataset",
                              "title": "Sameas"
                        },
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Place"
                        },
                        "geo": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/GeoCoordinates"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "GeoCoordinates of the place"
                        },
                        "eligibleRegion": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The ISO 3166-1 (ISO 3166-1 alpha-2) or ISO 3166-2 code, the place, or the GeoShape for the geo-political region(s) for which the offer or delivery charge specification is valid."
                        }
                  },
                  "title": "Place",
                  "type": "object"
            },
            "Ror": {
                  "pattern": "^https:\\/\\/ror\\.org.*$",
                  "title": "Ror",
                  "type": "string"
            },
            "SingleDate": {
                  "format": "date",
                  "title": "SingleDate",
                  "type": "string"
            },
            "Text": {
                  "title": "Text",
                  "type": "string"
            },
            "Text50": {
                  "maxLength": 5000,
                  "minLength": 50,
                  "title": "Text50",
                  "type": "string"
            },
            "TimePeriod": {
                  "pattern": "^\\d{4}-\\d{2}-\\d{2}\\/\\d{4}-\\d{2}-\\d{2}$",
                  "title": "TimePeriod",
                  "type": "string"
            },
            "hdr_schemata__models__SchemaOrg__GoogleRecommended__Organization__Organization": {
                  "properties": {
                        "name": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Name of the organisation",
                              "example": "Fictitious Research Consortium"
                        },
                        "identifier": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See background notes for more details."
                        },
                        "sameAs": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Ror"
                                    },
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Link or ror.org ID of the organisation",
                              "example": "https://ror.org/xxxxxxxxx",
                              "title": "Sameas"
                        },
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Organization"
                        },
                        "email": {
                              "anyOf": [
                                    {
                                          "format": "email",
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Email address.",
                              "title": "Email"
                        },
                        "legalName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Legal name of the organisation",
                              "example": "Fictitious Research Consortium"
                        }
                  },
                  "title": "Organization",
                  "type": "object"
            },
            "hdr_schemata__models__SchemaOrg__base__Organization__Organization": {
                  "properties": {
                        "name": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Name of the organisation",
                              "example": "Fictitious Research Consortium"
                        },
                        "identifier": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See background notes for more details."
                        },
                        "sameAs": {
                              "anyOf": [
                                    {
                                          "format": "uri",
                                          "minLength": 1,
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
                              "example": "https://www.kaggle.com/datasets/meirnizri/covid19-dataset",
                              "title": "Sameas"
                        },
                        "@type": {
                              "allOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    }
                              ],
                              "default": "Organization"
                        },
                        "email": {
                              "anyOf": [
                                    {
                                          "format": "email",
                                          "type": "string"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Email address.",
                              "title": "Email"
                        },
                        "legalName": {
                              "anyOf": [
                                    {
                                          "$ref": "#/$defs/Text"
                                    },
                                    {
                                          "type": "null"
                                    }
                              ],
                              "default": null,
                              "description": "Legal name of the organisation",
                              "example": "Fictitious Research Consortium"
                        }
                  },
                  "title": "Organization",
                  "type": "object"
            }
      },
      "additionalProperties": false,
      "description": "- inherits from the baseline Schema.org definitions (currently incomplete)\n- implements some additional Google Recommended specific patterns ",
      "properties": {
            "@type": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "Dataset"
            },
            "name": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "description": "A descriptive name of a dataset. For example, \"Snow depth in the Northern Hemisphere\".\n        Guidelines\n        \n        Use unique names for distinct datasets whenever possible.\n        Recommended: \"Snow depth in the Northern Hemisphere\" and \"Snow depth in the Southern Hemisphere\" for two different datasets.\n        \n        Not recommended: \"Snow depth\" and \"Snow depth\" for two different datasets.\n        ",
                  "title": "Name"
            },
            "identifier": {
                  "anyOf": [
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "items": {
                                    "anyOf": [
                                          {
                                                "format": "uri",
                                                "minLength": 1,
                                                "type": "string"
                                          },
                                          {
                                                "$ref": "#/$defs/Text"
                                          }
                                    ]
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "An identifier, such as a DOI or a Compact Identifier. If the dataset has more than one identifier, repeat the identifier property. If using JSON-LD, this is represented using JSON list syntax.",
                  "title": "identifier"
            },
            "creator": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/hdr_schemata__models__SchemaOrg__GoogleRecommended__Organization__Organization"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "The creator or author of this dataset. To uniquely identify individuals, use ORCID ID as the value of the sameAs property of the Person type. To uniquely identify institutions and organizations, use ROR ID.",
                  "example": [
                        {
                              "@type": "Person",
                              "familyName": "Foo",
                              "givenName": "Jane",
                              "name": "Jane Foo",
                              "sameAs": "https://orcid.org/0000-0000-0000-0000"
                        },
                        {
                              "@type": "Person",
                              "familyName": "Bar",
                              "givenName": "Jo",
                              "name": "Jo Bar",
                              "sameAs": "https://orcid.org/0000-0000-0000-0001"
                        },
                        {
                              "@type": "Organization",
                              "name": "Fictitious Research Consortium",
                              "sameAs": "https://ror.org/xxxxxxxxx"
                        }
                  ],
                  "title": "Dataset Creator"
            },
            "@id": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "Another ID for context"
            },
            "@context": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text"
                        }
                  ],
                  "default": "https://schema.org/"
            },
            "description": {
                  "allOf": [
                        {
                              "$ref": "#/$defs/Text50"
                        }
                  ],
                  "description": "A short summary describing a dataset.\n        Guidelines\n\n        The summary must be between 50 and 5000 characters long.\n        The summary may include Markdown syntax. Embedded images need to use absolute path URLs (instead of relative paths).\n        When using the JSON-LD format, denote new lines with \\n (two characters: backslash and lower case letter \"n\").\n        ",
                  "title": "description"
            },
            "distribution": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/DataDownload"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A downloadable form of this dataset, at a specific location, in a specific format. This property can be repeated if different variations are available. There is no expectation that different downloadable distributions must contain exactly equivalent information (see also DCAT on this point). Different distributions might include or exclude different subsets of the entire dataset, for example",
                  "title": "distribution"
            },
            "alternateName": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "items": {
                                    "$ref": "#/$defs/Text"
                              },
                              "type": "array"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Alternative names that have been used to refer to this dataset, such as aliases or abbreviations",
                  "example": [
                        "Quick Draw Dataset",
                        "quickdraw-dataset"
                  ],
                  "title": "Alternate Name"
            },
            "citation": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/Text"
                        },
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Identifies academic articles that are recommended by the data provider be cited in addition to the dataset itself. Provide the citation for the dataset itself with other properties, such as name, identifier, creator, and publisher properties. For example, this property can uniquely identify a related academic publication such as a data descriptor, data paper, or an article for which this dataset is supplementary material for. Don't use this property to provide citation information for the dataset itself. It is intended to identify related academic articles, not the dataset itself. To provide information necessary to cite the dataset itself use name, identifier, creator, and publisher properties instead.",
                  "examples": [
                        "https://doi.org/10.1111/111",
                        "https://identifiers.org/pubmed:11111111",
                        "https://identifiers.org/arxiv:0111.1111v1",
                        "Doe J (2014) Influence of X ... https://doi.org/10.1111/111"
                  ],
                  "title": "citation"
            },
            "funder": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/hdr_schemata__models__SchemaOrg__GoogleRecommended__Organization__Organization"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "A person or organization that provides financial support for this dataset. To uniquely identify individuals, use ORCID ID as the value of the sameAs property of the Person type. To uniquely identify institutions and organizations, use ROR ID.",
                  "examples": [
                        {
                              "@type": "Person",
                              "familyName": "Funder",
                              "givenName": "Jane",
                              "name": "Jane Funder",
                              "sameAs": "https://orcid.org/0000-0000-0000-0002"
                        },
                        {
                              "@type": "Organization",
                              "name": "Fictitious Funding Organization",
                              "sameAs": "https://ror.org/yyyyyyyyy"
                        }
                  ],
                  "title": "Funder"
            },
            "isPartOf": {
                  "anyOf": [
                        {
                              "format": "uri",
                              "minLength": 1,
                              "type": "string"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "If the dataset is a collection of smaller datasets, use the hasPart property to denote such relationship. Conversly, if the dataset is part of a larger dataset, use isPartOf. Both properties can take the form of a URL or a Dataset instance. In case Dataset is used as a value it has to include all of the properties required for a standalone Dataset. ",
                  "examples": [
                        "https://example.com/aggregate_dataset"
                  ],
                  "title": "Is Part Of (dataset)"
            },
            "hasPart": {
                  "anyOf": [
                        {
                              "$ref": "#/$defs/CreativeWork"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Indicates an item or CreativeWork that is part of this item, or CreativeWork (in some sense).",
                  "title": "Has Part"
            },
            "isAccessibleForFree": {
                  "anyOf": [
                        {
                              "type": "boolean"
                        },
                        {
                              "type": "null"
                        }
                  ],
                  "default": null,
                  "description": "Whether the dataset is accessible without payment.",
                  "title": "Is Accessible For Free?"
            },
            "keywords": {
                  "anyOf": [
                        {
